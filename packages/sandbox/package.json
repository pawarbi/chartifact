{
  "name": "@microsoft/chartifact-sandbox",
  "version": "1.0.0",
  "description": "Secure sandbox for Chartifact renderer",
  "type": "module",
  "main": "dist/esnext/index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf src/resources",
    "dev": "vite",
    "resources": "node scripts/resources.mjs",
    "prebuild": "npm run resources",
    "tsc": "tsc -p .",
    "bundle": "vite build --config vite.bundle.config.js",
    "build": "npm run tsc && npm run bundle",
    "build:04": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
